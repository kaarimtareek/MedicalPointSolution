@using System.Security.Claims;
@using MedicalPoint.Common;
@{
    if(ViewBag.PageName == null)
    {
        ViewBag.PageName = "قائد النقطة الطبية";
    }
    ViewBag.Name = Context.GetUserClaimValue( ClaimTypes.Name);
    ViewBag.UserId = Context.GetUserId().Value;
}
<!DOCTYPE html>
<html lang="ar" dir="rtl">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title> @ViewData["Title"] - MedicalPoint لوحه تحكم المنظومه </title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/LeaderDesign/assets/img/شعار الكلية.png" rel="icon">
    <link href="~/LeaderDesign/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/LeaderDesign/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/LeaderDesign/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/LeaderDesign/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/LeaderDesign/assets/vendor/quill/quill.snow.css" rel="stylesheet">
    <link href="~/LeaderDesign/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
    <link href="~/LeaderDesign/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
